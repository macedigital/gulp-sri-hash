{
  "name": "gulp-sri-hash",
  "version": "1.5.0",
  "description": "Gulp plugin for adding Sub-Resource-Integrity (SRI) hashes in-place to asset links found in HTML files.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "mocha",
    "test-cov": "istanbul cover _mocha -- 2> /dev/null",
    "test-travis": "istanbul cover _mocha --report lcovonly -- --reporter spec 2> /dev/null"
  },
  "keywords": [
    "gulp",
    "plugin",
    "sri",
    "sub-resource-integrity"
  ],
  "author": "Matthias Adler <macedigital@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/macedigital/gulp-sri-hash.git"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "gulp": "^3.9.1",
    "plugin-error": "^1.0.1",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.3",
    "stream-assert": "^2.0.3"
  },
  "engines": {
    "node": ">= 4.x"
  }
}
