{
  "name": "gulp-sri-hash",
  "version": "1.5.0",
  "description": "Gulp plugin for adding Sub-Resource-Integrity (SRI) hashes in-place to asset links found in HTML files.",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/macedigital/gulp-sri-hash/issues"
  },
  "homepage": "https://github.com/macedigital/gulp-sri-hash#readme",
  "scripts": {
    "pretest": "eslint .",
    "preversion": "git stash && npm test",
    "postversion": "git push && git push --tags && git stash pop",
    "test": "mocha test",
    "test-cov": "istanbul cover _mocha -- --reporter dot --check-leaks test/ 2> /dev/null",
    "test-travis": "istanbul cover _mocha --report lcovonly -- --reporter spec --check-leaks test/ 2> /dev/null"
  },
  "keywords": [
    "gulp",
    "plugin",
    "sri",
    "sub-resource-integrity"
  ],
  "author": "Matthias Adler <macedigital@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macedigital/gulp-sri-hash.git"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "plugin-error": "^1.0.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "gulp": "^3.9.1",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "stream-assert": "^2.0.3"
  },
  "engines": {
    "node": ">= 4.x"
  }
}
